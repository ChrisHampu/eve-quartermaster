{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "globals": {
    "__DEV__": true
  },
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "no-confusing-arrow": 0,
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "react/prefer-stateless-function": 0,
    "padded-blocks": 0,
    "max-len": 0,
    "comma-dangle": 0,
    "indent": 0,
    "object-shorthand": 0,
    "radix": 0,
    "vars-on-top": 0,
    "no-var": 0,
    "quotes": 0,
    "react/jsx-boolean-value": 0,
    "arrow-body-style": 0,
  }
}
